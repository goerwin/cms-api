{
  "name": "cms-api",
  "version": "1.0.0",
  "description": "Api for the CMS's Database",
  "engines": {
    "node": ">=10.15"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "eslint src/**/*.js && jest"
  },
  "author": "Erwin Gaitan O <erwingaitano@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongodb": "^3.3.5",
    "mongoose": "^5.7.14",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "jest": "^25.3.0",
    "mongodb-memory-server": "^6.2.0",
    "nodemon": "^2.0.1",
    "prettier": "1.19.1",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.js",
      "**/_tests/*.js",
      "**/_tests.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js"
    ],
    "testTimeout": 15000
  }
}
